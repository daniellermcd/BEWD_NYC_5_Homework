<%= render 'header' %>

<%= form_for @movie do |f| %>
<table class = "movie_form">
  <tr>
    <td><%= f.label 'title' %></td>
    <td><%= f.text_field 'title' %></td>
  </tr>
  <tr>
    <td><%= f.label 'description' %></td>
    <td><%= f.text_field 'description' %></td>
  </tr>
  <tr>
    <td><%= f.label 'year_released' %></td>
    <td><%= f.text_field 'year_released' %></td>
  </tr>
</table>
    <%= f.submit %> <%= link_to "Cancel", "/" %>
<% end %>